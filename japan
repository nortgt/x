AvatarTab:Section({Title = "Advanced Character Editor", Icon = "tag"})

bodyParts = {
    Torso = nil,
    RightArm = nil,
    LeftArm = nil,
    RightLeg = nil,
    LeftLeg = nil,
    Head = nil
}

AvatarTab:Input({
    Title = "Torso ‚úèÔ∏è",
    Desc = "Enter Torso ID",
    Callback = function(Value)
        if Value == "" then
            bodyParts.Torso = nil
            print("‚ö†Ô∏è Torso: empty")
        else
            local id = tonumber(Value) or 0
            bodyParts.Torso = id
            print("Torso: " .. id)
        end
    end
})

